{
  "name": "connection-ai",
  "version": "1.0.0",
  "description": "App that generates questions based on user input focusing on networking and relationships, using openAI API. Made for Quantum Connecting.",
  "engines": {
    "node": "16.18.0",
    "npm": "9.5.1"
  },
  "main": "server/index.js",
  "scripts": {
    "start": "node server/index.js",
    "develop": "concurrently \"cd server && npm run watch\" \"cd client && npm start\"",
    "install": "cd server && npm i && cd ../client && npm i",
    "seed": "cd server && npm run seed",
    "build": "cd client && npm run build"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^5.1.0"
  },
  "dependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.0",
    "eslint": "^8.42.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/js-Quest/connection-ai.git"
  },
  "bugs": {
    "url": "https://github.com/js-Quest/connection-ai/issues"
  },
  "homepage": "https://github.com/js-Quest/connection-ai#readme"
}
